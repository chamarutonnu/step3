<h1>投稿一覧</h1>

<div class="flex">
    <% @post_images.each do |post_image| %>
      <div>
        <%= link_to post_image_path(post_image.id), data: {turbolinks: false} do %>
        <%= image_tag "https://dmmpostimages.s3.ap-northeast-1.amazonaws.com/store/#{post_image.image_id}", :size =>'340x340' %>
        <% end %>
      </div>
    <% end %>
</div>
<div>
  <%= paginate @post_images %>
</div>



