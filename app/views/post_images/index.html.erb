<h1>投稿一覧</h1>

<div class="flex">
    <% @post_images.each do |post_image| %>
      <div>
        <%= link_to post_image_path(post_image.id) do %>
        <%= attachment_image_tag post_image, :image, :size =>'340x340' %>
        <% end %>
      </div>
    <% end %>
</div>
<div>
  <%= paginate @post_images %>
</div>



